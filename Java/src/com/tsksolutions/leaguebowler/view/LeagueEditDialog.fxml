<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" styleClass="background" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tsksolutions.leaguebowler.view.LeagueEditDialogController">
   <children>
      <GridPane AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="40.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="League Name" />
            <Label text="Bowling Center" GridPane.rowIndex="1" />
            <Label text="Has Handicap" GridPane.rowIndex="2" />
            <Label text="Handicap Target" GridPane.rowIndex="3" />
            <Label text="Handicap Percent" GridPane.rowIndex="4" />
            <TextField fx:id="leagueNameField" GridPane.columnIndex="1" />
            <TextField fx:id="sanctionCenterField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="hasHandicapField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="handicapTargetField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="handicapPercentField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
         </children>
      </GridPane>
      <Button defaultButton="true" mnemonicParsing="false" onAction="#handleOk" prefWidth="95.0" text="OK" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="105.0" />
      <Button cancelButton="true" mnemonicParsing="false" onAction="#handleCancel" prefWidth="95.0" text="Cancel" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5.0" />
   </children>
</AnchorPane>
